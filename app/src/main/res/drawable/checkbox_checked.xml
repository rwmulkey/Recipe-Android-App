<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:gravity="center|top">
        <shape
                android:shape="rectangle" >
            <size android:height="15dp" android:width="15dp"/>
            <solid
                    android:color="@android:color/transparent" />
            <stroke android:color="#635d5d" android:width="1dp"/>
        </shape>
    </item>
    <item
            android:width="8dp"
            android:height="1dp"
            android:top="11dp"
            android:left="8dp">
        <rotate
                android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#635d5d"/>
            </shape>
        </rotate>
    </item>

    <item
            android:width="18dp"
            android:height="1dp"
            android:top="7dp"
            android:left="12dp">
        <rotate
                android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="#635d5d"/>
            </shape>
        </rotate>
    </item>
</layer-list>